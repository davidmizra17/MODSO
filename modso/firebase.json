{
  "functions": [
    {
      "source": "functions",
      "codebase": "default"
    },
    {
      "source": "codebase",
      "codebase": "codebase",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "modso",
    "public": "src",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "heathers": [
      {
      "source": "*/.jsx",
      "heathers": [
        {
          "key": "Content-Type",
          "value": "text/jsx"
        }
      ]
      }
    ]
  }
}
